{
  "name": "yaksok",
  "version": "0.3.4",
  "description": "yaksok toolkit",
  "main": "dist/yaksok.min.js",
  "scripts": {
    "build": "babel-node ./build --dev && node ./dist/test.js",
    "debug": "babel-node ./build --dev && node debug ./dist/test.js",
    "clean": "babel-node ./build --clean",
    "watch": "babel-node ./build --watch --dev",
    "min": "babel-node ./build --min",
    "test": "npm run build",
    "prepublish": "node ./build/in-publish && npm run test && npm run min && npm run doc || node ./build/not-in-publish",
    "doc": "babel-node ./build/doc"
  },
  "preferGlobal": true,
  "bin": {
    "ysjs": "./bin/ysjs"
  },
  "author": {
    "name": "JongChan Choi",
    "email": "jong@chan.moe",
    "url": "http://0xABCDEF.com/"
  },
  "license": "Zlib",
  "repository": {
    "type": "git",
    "url": "http://github.com/disjukr/yaksok.js.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "fs-extra": "^0.26.4",
    "js-yaml": "^3.5.3",
    "lex-es6": "^1.1.0",
    "uuid": "^2.0.1",
    "yargs": "^3.31.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-3": "^6.5.0",
    "del": "^2.0.2",
    "jison-loader": "^1.0.0",
    "json-loader": "^0.5.3",
    "marked": "^0.3.5",
    "mocha": "^2.3.4",
    "raw-loader": "^0.5.1",
    "source-map-support": "^0.3.2",
    "webpack": "^1.12.2",
    "webpack-notifier": "^1.2.1"
  }
}
